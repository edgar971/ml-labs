{
    "build": {
        "dockerfile": "Dockerfile"
    },
    "hostRequirements": {
        "cpus": 4
    },
    "waitFor": "onCreateCommand",
    "customizations": {
        "codespaces": {
            "openFiles": []
        },
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter",
                "ms-python.python"
            ]
        }
    },
    "features": {
        "ghcr.io/devcontainers/features/python:1": {
            "installJupyterlab": true,
            "version": "3.8.5",
            "optimize": true
        }
    }
}
